DielectricRegion   //Top
{
	er = EPSR
	A1 = 0.0*plasmaLen
	A2 = plasmaRad
	B1 = plasmaLen
	B2 = sysRad+gThick

}


///////////////////
Gap      //Top
{
	A1 = 0.0*plasmaLen
	A2 = sysRad
	B1 = 0.0*plasmaLen
	B2 = sysRad-gThick
	normal = 1
	frequency = Freq
	A = 3e3
	C = 0
	EFFlag = 1
}


