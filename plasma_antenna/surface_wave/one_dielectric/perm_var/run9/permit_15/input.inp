Plasma_Antenna_TM
{
Plasma Antenna using ExitPort
}
Variables
{

dumpPeriod = 5
nTFinal = 2000000
nTStart = 1500000
// ******************************
// General physical parameters
// ******************************

  Q           = 1.60217662e-19
  mAr	        = 6.6335209E-26
  mE 	        = 9.11E-31
  c           = 2.9979e8
  EPS0        = 8.85418782e-12
  EPSR        = 15 // Dielectric value of Glass

  PLA_DEN     = 1E14
  KB          = 1.38e-23
  Te          = 2
  Ti 	        = 0.003
  TeK	        = Te*11604.525
  TiK         = Ti*11604.525
  Ni          = PLA_DEN
  Ne 	        = 1.0 * PLA_DEN
  DRIFTE      = 0.0 //1E6  % m/s
  TE          = sqrt(2*(Te*Q)/mE)//   % m/s sqrt(2*(Ti*Q)/mE)
  TI          = sqrt(2*(Ti*Q)/mAr)//   % m/s sqrt(2*(Ti*Q)/mE)
  wpe         = (8.98E3)*sqrt(Ne*1E-6)//
  PL_FREQ_E   = sqrt((Ne*Q*Q)/(EPS0*mE))/(2*pi)//
  ds          = 0.05 * c / PL_FREQ_E//
  Freq        = 3.2e9 //500E6
  DL          = sqrt((EPS0*TeK*KB)/(Ne*Q*Q)) //0.003   // Debye Length
  ppc         = 50


// ******************************
//	DEVICE PARAMETERS
// ******************************
  plasmaRad = 0.03          // plasma radius in meter
  gThick    = 0.01              // glass thickness
  sysRad    = plasmaRad + gThick   // system radius in meter
  plasmaLen = 0.3  //(DL*pLNum)           // system length in meter


// *************************
// 	SPATIAL Grid parameters
// *************************

  Nx = 64     //  plasmaLen/DL
  Ny = 32       // 2*sysRad/DL
  //dx =  DL
  //dy =  DL
  Lx =  plasmaLen
  Ly =  sysRad
  dx = Lx/Nx
  dy = Ly/Ny
  //simulationVolume = Lx * Ly
  //numCells         = Nx * Ny

  simulationVolume = Lx * Ly
  pNy              = plasmaRad/dy
  plasVolume       = plasmaRad*plasmaLen
  numCells         = Nx * pNy

// *************************
// streaming electrons
// *************************

  totalNumElectrons    = Ne * plasVolume
  numElectronsPerCell  = ppc
  numElectronPtcls     = numElectronsPerCell * numCells
  electronNumRatio     = totalNumElectrons / numElectronPtcls //np2c
  electronVelocityMKS  = DRIFTE
  peakCurrentElectrons = Q * Ne * Ly * electronVelocityMKS

// *************************
// TEMPORAL Grid parameters
// *************************

  d         = 1. / sqrt( 1./(dx*dx) + 1./(dy*dy) )
  simtimeStep = 0.2 * d / c
  timeStep  = (0.003 * d / c) * dumpPeriod


}

Region
{
Grid
{
	J   = Nx
	x1s = 0.0
	x1f = plasmaLen
	n1  = 1.0
	K   = Ny
	x2s = 0.0
	x2f = sysRad
	n2  = 1.0
	// Rule
	// {
	//  Limit
	//  n1 < 0.25
	//  Fatal -- n1 < 0.25 grid spacing too nonuniform to ensure accuracy
	// }
	// Rule
	// {
	//  Algebra
	//  J * K > 10000
	//  Warning -- J*K >= 10000 may mean memorElectric field near the sourcey problems!
	// }
	Geometry = 1
}
Control
{
	dt  = simtimeStep
	ElectrostaticFlag = 0
  particleLimit = 1E8
	FieldSubFlag = 25

}

MCC
{
	gas = Ar
	pressure = 0.1
	eSpecies = electrons
	iSpecies = argon
	collisionFlag=1
	ecxFactor = 1
	icxFactor = 1
}

Species
{
  name = electrons
  m = mE
  q = -Q
  collisionModel = None //1

}
Species
{
  name = argon
  m = mAr
  q = Q
  collisionModel = None //2
}

////////////////////////////////////////////// Boundary ///////////////
Conductor //Top
{

	A1 = 0.0*plasmaLen
	A2 = sysRad
	B1 = plasmaLen
	B2 = sysRad
	normal = -1
	// R = 1
	// C = 0
}

Conductor   //Left
{

	A1 = 0.0*plasmaLen
	A2 = 0.0
	B1 = 0.0*plasmaLen
	B2 = plasmaRad
	normal = 1
	// R = 1
	// C = 0
}

Conductor  //Right
{

	A1 = plasmaLen
	A2 = 0.0
	B1 = plasmaLen
	B2 = sysRad
	normal = -1
	// R = 1
	// C = 0
}

Conductor //Bottom
{

	A1 = 0.0*plasmaLen
	A2 = 0.0
	B1 = plasmaLen
	B2 = 0.0*sysRad
	normal = 1
 	// R = 1
	// C = 0
}


/////////////////// Load
Load
{
  v1drift = 0.0	// drift velocity in direction 1
  v2drift = 0.0	// drift velocity in direction 2
  v3drift = 0.0	// drift velocity in direction 3
  temperature = TE	// isotropic thermal velocity
  density = PLA_DEN	// uniform density of real particles
  x1MinMKS = 0.0	// minimun x1 postion to start load
  x1MaxMKS = plasmaLen	// maximun z postion to start load
  x2MinMKS = 0.0	// minimun r postion to start load
  x2MaxMKS = plasmaRad	// maximun r postion to start load
  np2c = electronNumRatio	// number of computer particles to real particles
  speciesName = electrons	// unique species name
//  LoadMethodFlag = QUIET_START	// Flag to control loading method, RANDOM or QUIET_START.
  units = MKS	// units of ALL velocities for maxwellian (MKS,EV)
}

Load
{
  v1drift = 0.0	// drift velocity in direction 1
  v2drift = 0.0	// drift velocity in direction 2
  v3drift = 0.0	// drift velocity in direction 3
  temperature = TI	// isotropic thermal velocity
  density = PLA_DEN	// uniform density of real particles
  x1MinMKS = 0.0	// minimun x1 postion to start load
  x1MaxMKS = plasmaLen	// maximun z postion to start load
  x2MinMKS = 0.0	// minimun r postion to start load
  x2MaxMKS = plasmaRad	// maximun r postion to start load
  np2c = electronNumRatio	// number of computer particles to real particles
  speciesName = argon	// unique species name
//  LoadMethodFlag = QUIET_START	// Flag to control loading method, RANDOM or QUIET_START.
  units = MKS	// units of ALL velocities for maxwellian (MKS,EV)
}

