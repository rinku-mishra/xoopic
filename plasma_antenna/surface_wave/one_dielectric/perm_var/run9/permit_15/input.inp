Diagnostic
{
  Comb = 1000
  Ave  = 10000
  HistMax = 1024
  title  = JdotE
  A1 = 0.0
  A2 = 0.0
  B1 = plasmaLen
  B2 = sysRad
  VarName = JdotE
  x1_Label = x
  x2_Label = y
  x3_Label = JdotE
}




